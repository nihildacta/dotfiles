{
  "languageserver": {
    "intelephense": {
        "command": "intelephense",
        "args": ["--stdio"],
        "filetypes": ["php"],
        "initializationOptions": {
            "storagePath": "/tmp/intelephense"
        }
     },
     "go": {
        "command": "gopls",
        "rootPatterns": ["go.mod"],
        "trace.server": "verbose",
        "filetypes": ["go"]
     }
  },
  "metals.statusBarEnabled": true,
  "coc.preferences.formatOnSaveFiletypes": ["scala", "go"],
  "metals.excludedPackages": ["akka.actor.typed.javadsl"],
  "metals.serverVersion": "0.9.7"
}
